{
	/*
	// Place your snippets for HTML here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
	"<tagName>{new line}</tagName>": {
		"prefix": "tt",
		"body": "<${1:div}>\n\t$0\n</${1}>",
		"description": "create a pair of multi line tags"
	},
	// "view-ref": {
	// 	"prefix": "ref",
	// 	"body": "view-ref=\"${1:someId}\" "
	// },
	// "view-name": {
	// 	"prefix": "name",
	// 	"body": "view-name=\"${1:someName}\" "
	// },
	"<section></section>": {
		"prefix": "section",
		"body": "<section$1>\n\t$0\n</section>"
	},
	"<img></img>": {
		"prefix": "img",
		"body": "<img src=\"$1\">"
	},
	"<a></a>": {
		"prefix": "a",
		"body": "<a href=\"$1\">$0</a>"
	},
	"<script src></script>": {
		"prefix": "script",
		"body": "<script src=\"$1\" charset=\"utf-8\"></script>"
	},
	"<input />": {
		"prefix": "input",
		"body": "<input type=\"$1\"/>"
	},
	"class=\"\"": {
		"prefix": "class",
		"body": "class=\"$1\"$0"
	},
	"id=\"\"": {
		"prefix": "id",
		"body": "id=\"$1\"$0"
	},
	"style=\"display:none;\"": {
		"prefix": "hidden",
		"body": "style=\"display:none;\""
	},
	"target=\"_blank\"": {
		"prefix": "target",
		"body": "target=\"_blank\""
	},
	// for wxml
	"wxif": {
		"prefix": "wx.if",
		"body": "wx:if=\"{{$1}}\""
	},
	"wxelse": {
		"prefix": "wx.else",
		"body": "wx:else"
	},
	"wxfor": {
		"prefix": "wx.for",
		"body": "wx:for=\"{{$1}}\""
	},
	"wxforitem": {
		"prefix": "wx.foritem",
		"body": "wx:for-item=\"{{$1}}\""
	},
	"{% jinja %}": {
		"prefix": "%",
		"body": "{% ${block} %}"
	},
	"{% jinja_cmd %}{% end%}": {
		"prefix": "%%",
		"body": "{% ${1:if} $2 %}$0{% end$1 %}"
	},
	"<For></For>": {
		"prefix": "For",
		"body": "<For list=\"${1:list}\" item=\"${2:item}\" ${3:index=\"${4:index}\"}>\n\t$0\n</For>",
	},
	"<Subview></Subview>": {
		"prefix": "Subview",
		"body": "<Subview is=\"${1:ViewClass}\" props=\"${2:data}\">$0</Subview>",
	},
	"<If></If>": {
		"prefix": "If",
		"body": "<If cond=\"$1\">$0</If>"
	},
	"<Elif/>": {
		"prefix": "Elif",
		"body": "<Elif cond=\"$1\" />"
	},
	"<Else/>": {
		"prefix": "Else",
		"body": "<Else />"
	},
	"<Slot></Slot>": {
		"prefix": "Slot",
		"body": "<Slot name=\"${1:slotName}\" ${2:default}>$0</Slot>"
	},
	"<Plugin></Plugin>": {
		"prefix": "Plugin",
		"body": "<Plugin slot=\"${1:slotName}\">$0</Plugin>"
	},
	"@ref=\"\"": {
		"prefix": "@ref",
		"body": "@ref=\"$2\""
	},
	"@attr:name=\"\"": {
		"prefix": "@attr",
		"body": "@attr:${1:name}=\"$2\""
	},
	"@bind:event=\"\"": {
		"prefix": "@bind",
		"body": "@bind:${1:click}=\"${2:handleClick}\""
	},
	"@catch:event=\"\"": {
		"prefix": "@catch",
		"body": "@catch:${1:click}=\"${2:handleClick}\""
	},
	"@value=\"\"": {
		"prefix": "@value",
		"body": "@value=\"${1:value}\""
	},
	"<Icon name=\"\"></Icon>": {
		"prefix": "Icon",
		"body": "<Icon name=\"$1\"></Icon>"
	}
}